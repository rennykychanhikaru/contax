      "@kit/ui/utils": ["../../next-supabase-saas-kit-turbo/packages/ui/src/lib/utils/index.ts"],
      "@kit/ui/card": ["../../next-supabase-saas-kit-turbo/packages/ui/src/shadcn/card.tsx"]
