      "@kit/ui/card": ["../../next-supabase-saas-kit-turbo/packages/ui/src/shadcn/card.tsx"],
      "@kit/ui/checkbox": ["../../next-supabase-saas-kit-turbo/packages/ui/src/shadcn/checkbox.tsx"],
      "@kit/ui/label": ["../../next-supabase-saas-kit-turbo/packages/ui/src/shadcn/label.tsx"],
      "@kit/ui/select": ["../../next-supabase-saas-kit-turbo/packages/ui/src/shadcn/select.tsx"],
      "@kit/ui/badge": ["../../next-supabase-saas-kit-turbo/packages/ui/src/shadcn/badge.tsx"],
      "@kit/ui/alert": ["../../next-supabase-saas-kit-turbo/packages/ui/src/shadcn/alert.tsx"],
